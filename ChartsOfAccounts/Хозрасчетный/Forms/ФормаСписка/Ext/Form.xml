				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>Субконто</dcssch:dataPath>
					<dcssch:field>Субконто</dcssch:field>
					<dcssch:title xsi:type="v8:LocalStringType">
						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>Субконто</v8:content>
