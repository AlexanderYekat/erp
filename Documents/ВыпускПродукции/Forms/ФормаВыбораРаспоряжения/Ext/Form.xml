			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	Остатки.Распоряжение.Номер       КАК Номер,
	Остатки.Распоряжение.Дата        КАК Дата,
	Остатки.Распоряжение.Статус      КАК Статус,
	Остатки.Распоряжение             КАК Ссылка,
	Остатки.Подразделение            КАК Подразделение,
	Остатки.Распоряжение.Организация КАК Организация
ИЗ
	РегистрНакопления.РаспоряженияНаВыпускПродукции.Остатки(,
		Распоряжение.Распоряжение.НаправлениеДеятельности = &amp;НаправлениеДеятельности
		И ВЫБОР КОГДА &amp;НаправлениеВыпуска = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукцииНаСклад) ТОГДА
					Склад = &amp;Склад
						И Номенклатура.ТипНоменклатуры В(
							ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар),
							ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.МногооборотнаяТара))
				ИНАЧЕ
					НЕ Склад ССЫЛКА Справочник.Склады
			КОНЕЦ) КАК Остатки
			
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.РаспоряженияНаВыпускПродукции КАК Движения
			ПО Движения.Регистратор = &amp;Ссылка
			 И Движения.Активность
			 И Движения.Распоряжение.Распоряжение.НаправлениеДеятельности = &amp;НаправлениеДеятельности
			 И ВЫБОР КОГДА &amp;НаправлениеВыпуска = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукцииНаСклад) ТОГДА
					Движения.Склад = &amp;Склад
						И Движения.Номенклатура.ТипНоменклатуры В(
							ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар),
							ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.МногооборотнаяТара))
				ИНАЧЕ
					НЕ Движения.Склад ССЫЛКА Справочник.Склады
			КОНЕЦ
			И Движения.Распоряжение = Остатки.Распоряжение
			И Движения.Подразделение = Остатки.Подразделение
			
	ГДЕ
		Движения.Регистратор ЕСТЬ NULL
	
ОБЪЕДИНИТЬ ВСЕ
	
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Движения.Распоряжение.Номер       КАК Номер,
	Движения.Распоряжение.Дата        КАК Дата,
	Движения.Распоряжение.Статус      КАК Статус,
	Движения.Распоряжение             КАК Ссылка,
	Движения.Подразделение            КАК Подразделение,
	Движения.Распоряжение.Организация КАК Организация
ИЗ
	РегистрНакопления.РаспоряженияНаВыпускПродукции КАК Движения
ГДЕ
	Движения.Регистратор = &amp;Ссылка
		И Движения.Активность
		И Движения.Распоряжение.Распоряжение.НаправлениеДеятельности = &amp;НаправлениеДеятельности
		И ВЫБОР КОГДА &amp;НаправлениеВыпуска = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукцииНаСклад) ТОГДА
					Движения.Склад = &amp;Склад
						И Движения.Номенклатура.ТипНоменклатуры В(
							ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Товар),
							ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.МногооборотнаяТара))
				ИНАЧЕ
					НЕ Движения.Склад ССЫЛКА Справочник.Склады
			КОНЕЦ</QueryText>
